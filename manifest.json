{
  "manifest_version": 2,
  "name": "Slick RSS",
  "version": "3.000",
  "description": "A full featured RSS reader that's fully contained within the browser.",
  "options_page": "options.html",
  "icons": {
    "128": "icon_128.png",
    "16": "transmit.png"
  },
  "browser_action": {
    "default_icon": "transmit.png",
    "default_title": "Slick RSS"
  },
  "background": {
    "scripts": [
      "sha256.js",
      "common.js",
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "http://*/",
    "https://*/",
    "tabs",
    "bookmarks"
  ],
  "content_security_policy": "script-src 'self'; object-src 'self'"
}
