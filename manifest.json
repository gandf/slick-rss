{
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "version": "3.001",
  "description": "__MSG_appDesc__",
  "options_page": "options.html",
  "default_locale": "en",
  "icons": {
    "128": "icon_128.png",
    "16": "transmit.png"
  },
  "browser_action": {
    "default_icon": "transmit.png",
    "default_title": "Slick RSS"
  },
  "background": {
    "scripts": [
      "sha256.js",
      "common.js",
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "http://*/",
    "https://*/",
    "activeTab"
  ],
  "content_security_policy": "script-src 'self'; object-src 'self'"
}
